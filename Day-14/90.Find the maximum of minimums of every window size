#include<bits/stdc++.h>
vector<int> maxMinWindow(vector<int> a, int n) {
    // Write your code here.
    vector<int> ans;
    stack<int> s;
    vector<int> left,right;
    for(int i=0;i<n;i++){
        left.push_back(-1);
        right.push_back(n);
    }
    for(int i=0;i<n;i++){
        while(!s.empty() && a[s.top()]>=a[i])
            s.pop();
        if(!s.empty())
            left[i]=s.top();
        s.push(i);
    }
    while(!s.empty())
        s.pop();
    for(int i=n-1;i>=0;i--){
        while(!s.empty() && a[s.top()]>=a[i])
            s.pop();
        if(!s.empty())
            right[i]=s.top();
        s.push(i);
    }
    for(int i=0;i<=n;i++)
        ans.push_back(INT_MIN);
    for (int i=0; i<n; i++)
    {
        int len = right[i] - left[i] - 1;
        ans[len] = max(ans[len], a[i]);
    }
    for (int i=n-1; i>=1; i--)
        ans[i] = max(ans[i], ans[i+1]);
    ans.erase(ans.begin());
    return ans;
}